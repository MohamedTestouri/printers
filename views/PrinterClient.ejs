            </nav>
            <!-- End Navbar -->
            <div class="content">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-md-12">
                            <a class="btn btn-primary pull-center"
                                href="/client/facture/show/<%=client._id%>">Factures</a>
                            <div class="card">
                                <div class="card-header card-header-primary">
                                    <h4 class="card-title ">Printers Table</h4>
                                    <p class="card-category"> Here is a subtitle for this table</p>
                                </div>
                                <div class="card-body">
                                    <div class="table-responsive">
                                        <table class="table">
                                            <thead class=" text-primary">
                                                <th>
                                                    Printer
                                                </th>
                                                <th>
                                                    N°Série
                                                </th>
                                                <th>
                                                    N°Article
                                                </th>

                                                <th>
                                                    Etat
                                                </th>
                                                <th>
                                                    Nombre de papier
                                                </th>
                                                <th>@IP</th>
                                                <th>
                                                    Date d'installation
                                                </th>
                                                <th>

                                                </th>

                                            </thead>
                                            <tbody>
                                                <% clients.printers.forEach(function(printer){ %>
                                                    <tr>
                                                        <td>
                                                            <%=printer.idPrinter %>
                                                        </td>
                                                        <td>
                                                            <%=printer.serialNumber %>
                                                        </td>
                                                        <td>
                                                            <%=printer.articleNumber%>
                                                        </td>
                                                        <td>
                                                            <%=printer.ip %>
                                                        </td>
                                                        <td>
                                                            <%=printer.location %>
                                                        </td>
                                                        <td>
                                                            <%=printer.state %>
                                                        </td>
                                                        <td>
                                                            <%=printer.installationDate %>
                                                        </td>
                                                        <td>
                                                            <%=printer.paperSum %>
                                                        </td>
                                                        <td>
                                                            <%=printer.currentA4Black %>
                                                        </td>
                                                        <td>
                                                            <%=printer.currentA4Color %>
                                                        </td>
                                                        <td>
                                                            <%=printer.currentA3Black %>
                                                        </td>
                                                        <td>
                                                            <%=printer.currentA3Color %>
                                                        </td>
                                                        <td>
                                                            <%=printer.creationDate %>
                                                        </td>
                                                        <td>
                                                            <%=printer.percBlack %>
                                                        </td>
                                                        <td>
                                                            <%=printer.percMagenta %>
                                                        </td>
                                                        <td>
                                                            <%=printer.percYellow %>
                                                        </td>
                                                        <td>
                                                            <%=printer.percBlackDrum %>
                                                        </td>
                                                        <td>
                                                            <%=printer.percCyanDrum %>
                                                        </td>
                                                        <td>
                                                            <%=printer.percYellowDrum %>
                                                        </td>
                                                        <td>
                                                            <%=printer.percMagentaDrum %>
                                                        </td>

                                                    </tr>
                                                    <% }); %>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <footer class="footer">
                <div class="container-fluid">
                </div>
            </footer>
        </div>
